@model ShiftCaptain.Models.Clone

<div class="row">
    <div class="small-12 large-12 columns">
        <h3>Users</h3>
    </div>
    <div class="small-12 large-5 columns">
        @Html.ListBox("users", Model.User)
    </div>
    <div class="small-12 large-2 columns">
        <input type="button" id="btnCloneAllUsers" value=">>" />
        <input type="button" id="btnCloneUser" value=">" />
        <input type="button" id="btnRemoveUser" value="<" />
        <input type="button" id="btnRemoveAllUsers" value="<<" />
    </div>        
    <div class="small-12 large-5 columns">
        @Html.ListBoxFor(model => model.CloneUser, Model.SelectedUsers, new { })
    </div>
</div>
